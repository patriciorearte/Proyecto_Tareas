{
    "version": 2,
    "builds": [
      {
        "src": "src/frontend/package.json",
        "use": "@vercel/react"
      },
      {
        "src": "src/backend/app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/frontend/index.html"
      }
    ]
  }